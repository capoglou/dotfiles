#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'terminalmdb==0.0.3','console_scripts','imdb'
__requires__ = 'terminalmdb==0.0.3'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('terminalmdb==0.0.3', 'console_scripts', 'imdb')()
    )
